<Window x:Class="Cars_Reporting.WindowRptCentrePostingDate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Centre Posting Date" Height="168" Width="300">
    <Grid>
        <Label Content="Select Month" Height="28" HorizontalAlignment="Left" Margin="18,41,0,0" Name="labelSelectMonth" VerticalAlignment="Top" />
        <ComboBox ItemsSource="{Binding}" Height="23" HorizontalAlignment="Left" Margin="96,12,0,0" Name="comboBoxYear" VerticalAlignment="Top" Width="68" />
        <ComboBox ItemsSource="{Binding}" Height="23" HorizontalAlignment="Left" Margin="96,41,0,0" Name="comboBoxMonth" VerticalAlignment="Top" Width="68" />
        <Label Content="Select Year" Height="28" HorizontalAlignment="Left" Margin="18,12,0,0" Name="labelSelectYear" VerticalAlignment="Top" />
        <Button Content="Generate" Height="23" HorizontalAlignment="Left" Margin="180,41,0,0" Name="buttonGenerate" VerticalAlignment="Top" Width="75" IsDefault="True" />
        <Button Content="Close" Height="23" HorizontalAlignment="Left" Margin="180,93,0,0" Name="buttonClose" VerticalAlignment="Top" Width="75" IsCancel="True" />
    </Grid>
</Window>
